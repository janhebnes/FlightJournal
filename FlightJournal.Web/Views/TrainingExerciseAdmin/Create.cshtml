@model FlightJournal.Web.Models.Training.Catalogue.Training2Exercise

@{
    ViewBag.Title = _("Create");
}
<h3>@ViewBag.TrainingProgramName - @ViewBag.TrainingLessonName</h3>
<h2>@_("Create exercise")</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)
    <fieldset>
        @Html.Partial("_PartialTrainingExercise", Model)

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.Partial("_PartialBackToList", Model)
</div>
